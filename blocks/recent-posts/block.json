{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "acf/recent-posts",
    "version": "0.1.0",
	"title": "Últimas Entradas del Blog",
	"category": "theme",
	"icon": "admin-post",
	"description": "Muestra automáticamente las entradas más recientes del blog",
	"keywords": ["blog", "posts", "entradas", "recientes", "últimas"],
	"supports": {
		"align": ["wide", "full"],
		"anchor": true,
		"customClassName": true,
		"jsx": true
	},
    "acf": {
        "mode": "preview",
        "renderTemplate": "render.php",
        "validate": false,
        "blockVersion": 3
    },
	"example": {
		"attributes": {
			"mode": "preview",
			"data": {
				"posts_count": "3"
			}
		}
	},
	"style": "file:./style.css",
	"editorStyle": "file:./editor.css"
}