{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "acf/form",
  "version": "0.1.0",
  "title": "Formulario",
  "category": "theme",
  "icon": "email-alt",
  "description": "Bloque de formulario con imagen, texto y formulario de contacto",
  "keywords": ["formulario", "contacto", "form", "email"],
  "supports": {
    "align": ["wide", "full"],
    "anchor": true,
    "className": true,
    "spacing": { "margin": false, "padding": false },
    "color": { "background": false, "text": false, "gradients": false },
    "typography": { "fontSize": false, "lineHeight": false },
    "shadow": false
  },
  "styles": [
    { "name": "default", "label": "Por defecto", "isDefault": true },
    { "name": "rounded", "label": "Imagen redondeada" },
    { "name": "shadow", "label": "Con sombra" }
  ],
  "acf": {
    "mode": "preview",
    "renderTemplate": "render.php",
    "validate": false,
    "blockVersion": 3
  },
  "example": {
    "attributes": {
      "heading": "Â¿Necesitas ayuda?",
      "subheading": "Contacto",
      "body": "Completa el formulario y nos pondremos en contacto contigo lo antes posible.",
      "layout_variant": "text-left",
      "form_id": 1
    }
  },
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}