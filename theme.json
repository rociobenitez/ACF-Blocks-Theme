	"styles": {
		"color": {
			"text": "var(--wp--preset--color--gray-800)",
			"background": "var(--wp--preset--color--white)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"elements": {
			"button": {
				"color": {
					"text": "var(--wp--preset--color--white)",
					"background": "var(--wp--preset--color--primary)"
				},
				"border": {
					"radius": "0.5rem"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--white)",
						"background": "var(--wp--preset--color--primary-dark)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary-dark)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--gray-900)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"lineHeight": "1.2"
				}
			}
		},
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--white)",
					"background": "var(--wp--preset--color--gray-800)"
				},
				"border": {
					"radius": "0.5rem"
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--gray-700)"
				},
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"width": "0 0 0 4px"
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var(--wp--preset--color--gray-700)",
					"background": "var(--wp--preset--color--gray-50)"
				}
			}
		}
